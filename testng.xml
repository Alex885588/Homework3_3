<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests">
    <test name="1">
        <parameter name="browser" value="chrome">
            <classes>
                <class name="Tests">
                    <methods>
                        <include name="testForUrl"/>
                    </methods>
                </class>
            </classes>
        </parameter>
    </test>
    <test name="2">
        <parameter name="browser" value="chrome">
            <classes>
                <class name="Tests">
                    <methods>
                        <include name="testFromPageToPage"/>
                    </methods>
                </class>
            </classes>
        </parameter>
    </test>
    <test name="3">
        <parameter name="browser" value="chrome">
            <classes>
                <class name="Tests">
                    <methods>
                        <include name="testForLogo"/>
                    </methods>
                </class>
            </classes>
        </parameter>
    </test>
    <test name="4">
        <parameter name="browser" value="chrome">
            <classes>
                <class name="Tests">
                    <methods>
                        <include name="testForImage"/>
                    </methods>
                </class>
            </classes>
        </parameter>
    </test>
</suite>